<script>
function toggleFaq(id) {
  const answer = document.getElementById(`answer-${id}`);
  const icon = document.getElementById(`icon-${id}`);
  
  if (answer.classList.contains('hidden')) {
    answer.classList.remove('hidden');
    icon.style.transform = 'rotate(180deg)';
  } else {
    answer.classList.add('hidden');
    icon.style.transform = 'rotate(0deg)';
  }
}

function toggleTestimonials() {
  const moreTestimonials = document.getElementById('more-testimonials');
  const btnText = document.getElementById('btn-text');
  const chevronIcon = document.getElementById('chevron-icon');
  
  if (moreTestimonials.classList.contains('hidden')) {
    moreTestimonials.classList.remove('hidden');
    btnText.textContent = 'Show Less Testimonials';
    chevronIcon.style.transform = 'rotate(180deg)';
  } else {
    moreTestimonials.classList.add('hidden');
    btnText.textContent = 'Show More Testimonials';
    chevronIcon.style.transform = 'rotate(0deg)';
  }
}
</script>

<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

<script src="https://unpkg.com/lucide@latest"></script>

<script>
    lucide.createIcons();
    AOS.init({
        once: true,
        duration: 800,
        easing: 'ease-out',
        offset: 50,
    });
</script>

